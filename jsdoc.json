{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/typescript"
  ],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "Movies Explorer"
    }
  }
}